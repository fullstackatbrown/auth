{
    "name": "Auth",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/auth",
    "forwardPorts": [
        8000,
        3000,
        27017
    ],
    "portsAttributes": {
        "8000": {
            "label": "Server",
            "onAutoForward": "silent"
        },
        "3000": {
            "label": "Client",
            "onAutoForward": "silent"
        },
        "27017": {
            "label": "DB",
            "onAutoForward": "silent"
        }
    },
    "otherPortsAttributes": {
        "onAutoForward": "silent"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.go",
                "dbaeumer.vscode-eslint",
                "mongodb.mongodb-vscode",
                "42Crunch.vscode-openapi",
                "redhat.vscode-yaml"
            ]
        }
    }
}
